const Button = ()=>{
    let rgb = {3,4,5}
    const sendRgb = (e) =>{
        e.preventDefault();     
    try {
        let res = fetch("http://5000/color/:rgb)
        .then((res)=>res.json())
        .then((data) =>{
            console.log(data)
    })} catch (err) {
        console.log("error!")
    }

    return (
        <div><button onClick=sendRgb>send</button></div>
    )
}


export default Button;
